<!DOCTYPE html>
<html>
  <head>
    <title>Healthcare Management</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= image_tag('/assets/image.jpg', width: '100px') %>

  <nav class="navbar navbar-light navbar-toggleable-xl bg-faded">
    <nav class="navbar navbar-default">
        <div class="navbar-header">
          <a class="navbar-brand" href="http://0.0.0.0:3000/">HealthCare</a>
<ul class="navbar-nav">
<li class="nav-item">

    <a class="nav-link" href="http://0.0.0.0:3000/patients">Patients</a>
</li>
  <li class="nav-item">
      <a class="nav-link" href="http://0.0.0.0:3000/doctors">Doctors</a>
  </li>
  <li class="nav-item">
      <a class="nav-link" href="http://0.0.0.0:3000/clinics">Clinics</a>
  </li>
  <li class="nav-item">
      <a class="nav-link" href="http://0.0.0.0:3000/hospitals">Hospitals</a>
  </li>
</ul>
</nav>
    <%= render "layouts/header" %>
    <div class="container-fluid">
      <%= yield %>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
    </div>
  </body>
</html>
